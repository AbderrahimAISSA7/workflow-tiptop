{
  "id": null,
  "uid": "workflow-tiptop",
  "title": "Workflow Furious Ducks",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Load Jenkins",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "avg(rate(process_cpu_seconds_total{job=\"jenkins\"}[5m]))",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "graph",
      "title": "CPU containers TipTop",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{container=~\"(front|api).*\"}[5m])) by (container)",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    },
    {
      "type": "graph",
      "title": "Utilisation m√©moire",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "sum(container_memory_usage_bytes{container=~\"(front|api).*\"}) by (container)",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "prometheus" }
    }
  ]
}
